@prefix DPDO: <http://www.foo.bar/LLM4BI/ontologies/LLM4BI#> .
@prefix LLM4BI_Customer: <http://www.foo.bar/LLM4BI/ontologies/LLM4BI_Customer#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

LLM4BI_Customer: a owl:Ontology .

LLM4BI_Customer:Customer a DPDO:Fact ;
    DPDO:determines LLM4BI_Customer:Customer_Area,
        LLM4BI_Customer:Customer_Billing_Information,
        LLM4BI_Customer:Customer_City,
        LLM4BI_Customer:Customer_Country,
        LLM4BI_Customer:Customer_County,
        LLM4BI_Customer:Customer_Customer,
        LLM4BI_Customer:Customer_Shipping_Address,
        LLM4BI_Customer:Customer_State .

LLM4BI_Customer:Customer_Area a DPDO:Attribute ;
    DPDO:isConvergence false ;
    DPDO:isCrossDimensional false ;
    DPDO:isDescriptive false ;
    DPDO:isOptional false ;
    DPDO:isShared false ;
    DPDO:itemName "Area"^^xsd:string .

LLM4BI_Customer:Customer_Billing_Information a DPDO:Attribute ;
    DPDO:isConvergence false ;
    DPDO:isCrossDimensional false ;
    DPDO:isDescriptive true ;
    DPDO:isOptional false ;
    DPDO:isShared false ;
    DPDO:itemName "Billing Information"^^xsd:string .

LLM4BI_Customer:Customer_City a DPDO:Attribute ;
    DPDO:isConvergence false ;
    DPDO:isCrossDimensional false ;
    DPDO:isDescriptive false ;
    DPDO:isOptional false ;
    DPDO:isShared false ;
    DPDO:itemName "City"^^xsd:string .

LLM4BI_Customer:Customer_Country a DPDO:Attribute ;
    DPDO:isConvergence false ;
    DPDO:isCrossDimensional false ;
    DPDO:isDescriptive false ;
    DPDO:isOptional false ;
    DPDO:isShared false ;
    DPDO:itemName "Country"^^xsd:string .

LLM4BI_Customer:Customer_County a DPDO:Attribute ;
    DPDO:isConvergence false ;
    DPDO:isCrossDimensional false ;
    DPDO:isDescriptive false ;
    DPDO:isOptional true ;
    DPDO:isShared false ;
    DPDO:itemName "County"^^xsd:string .

LLM4BI_Customer:Customer_Customer a DPDO:Attribute ;
    DPDO:isConvergence false ;
    DPDO:isCrossDimensional false ;
    DPDO:isDescriptive false ;
    DPDO:isOptional false ;
    DPDO:isShared false ;
    DPDO:itemName "Customer"^^xsd:string .

LLM4BI_Customer:Customer_Shipping_Address a DPDO:Attribute ;
    DPDO:isConvergence false ;
    DPDO:isCrossDimensional false ;
    DPDO:isDescriptive true ;
    DPDO:isOptional false ;
    DPDO:isShared false ;
    DPDO:itemName "Shipping Address"^^xsd:string .

LLM4BI_Customer:Customer_State a DPDO:Attribute ;
    DPDO:isConvergence false ;
    DPDO:isCrossDimensional false ;
    DPDO:isDescriptive false ;
    DPDO:isOptional true ;
    DPDO:isShared false ;
    DPDO:itemName "State"^^xsd:string .

