@prefix DPDO: <http://www.foo.bar/LLM4BI/ontologies/LLM4BI#> .
@prefix LLM4BI_Time: <http://www.foo.bar/LLM4BI/ontologies/LLM4BI_Time#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

LLM4BI_Time: a owl:Ontology .

LLM4BI_Time:Time a DPDO:Fact ;
    DPDO:determines LLM4BI_Time:Date_4_Month_Period,
        LLM4BI_Time:Date_Date,
        LLM4BI_Time:Date_Month,
        LLM4BI_Time:Date_Month_In_Year,
        LLM4BI_Time:Date_Quarter,
        LLM4BI_Time:Date_Semester,
        LLM4BI_Time:Date_Week,
        LLM4BI_Time:Date_Year .

LLM4BI_Time:Date_4_Month_Period a DPDO:Attribute ;
    DPDO:determines LLM4BI_Time:Date_Year ;
    DPDO:isConvergence false ;
    DPDO:isCrossDimensional false ;
    DPDO:isDescriptive false ;
    DPDO:isOptional false ;
    DPDO:isShared false ;
    DPDO:itemName "4-Month Period"^^xsd:string .

LLM4BI_Time:Date_Date a DPDO:Attribute ;
    DPDO:isConvergence false ;
    DPDO:isCrossDimensional false ;
    DPDO:isDescriptive false ;
    DPDO:isOptional false ;
    DPDO:isShared false ;
    DPDO:itemName "Date"^^xsd:string .

LLM4BI_Time:Date_Month a DPDO:Attribute ;
    DPDO:isConvergence false ;
    DPDO:isCrossDimensional false ;
    DPDO:isDescriptive false ;
    DPDO:isOptional false ;
    DPDO:isShared false ;
    DPDO:itemName "Month"^^xsd:string .

LLM4BI_Time:Date_Month_In_Year a DPDO:Attribute ;
    DPDO:isConvergence false ;
    DPDO:isCrossDimensional false ;
    DPDO:isDescriptive false ;
    DPDO:isOptional false ;
    DPDO:isShared false ;
    DPDO:itemName "Month In Year"^^xsd:string .

LLM4BI_Time:Date_Quarter a DPDO:Attribute ;
    DPDO:isConvergence false ;
    DPDO:isCrossDimensional false ;
    DPDO:isDescriptive false ;
    DPDO:isOptional false ;
    DPDO:isShared false ;
    DPDO:itemName "Quarter"^^xsd:string .

LLM4BI_Time:Date_Semester a DPDO:Attribute ;
    DPDO:determines LLM4BI_Time:Date_Year ;
    DPDO:isConvergence false ;
    DPDO:isCrossDimensional false ;
    DPDO:isDescriptive false ;
    DPDO:isOptional false ;
    DPDO:isShared false ;
    DPDO:itemName "Semester"^^xsd:string .

LLM4BI_Time:Date_Week a DPDO:Attribute ;
    DPDO:isConvergence false ;
    DPDO:isCrossDimensional false ;
    DPDO:isDescriptive false ;
    DPDO:isOptional false ;
    DPDO:isShared false ;
    DPDO:itemName "Week"^^xsd:string .

LLM4BI_Time:Date_Year a DPDO:Attribute ;
    DPDO:isConvergence true ;
    DPDO:isCrossDimensional false ;
    DPDO:isDescriptive false ;
    DPDO:isOptional false ;
    DPDO:isShared false ;
    DPDO:itemName "Year"^^xsd:string .

