@prefix DPDO: <http://www.foo.bar/LLM4BI/ontologies/LLM4BI#> .
@prefix LLM4BI_Geography: <http://www.foo.bar/LLM4BI/ontologies/LLM4BI_Geography#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

LLM4BI_Geography: a owl:Ontology .

LLM4BI_Geography:Geography a DPDO:Fact ;
    DPDO:determines LLM4BI_Geography:City_Area,
        LLM4BI_Geography:City_City,
        LLM4BI_Geography:City_Country,
        LLM4BI_Geography:City_County,
        LLM4BI_Geography:City_State .

LLM4BI_Geography:City_Area a DPDO:Attribute ;
    DPDO:isConvergence false ;
    DPDO:isCrossDimensional false ;
    DPDO:isDescriptive false ;
    DPDO:isOptional false ;
    DPDO:isShared false ;
    DPDO:itemName "Area"^^xsd:string .

LLM4BI_Geography:City_City a DPDO:Attribute ;
    DPDO:isConvergence false ;
    DPDO:isCrossDimensional false ;
    DPDO:isDescriptive false ;
    DPDO:isOptional false ;
    DPDO:isShared false ;
    DPDO:itemName "City"^^xsd:string .

LLM4BI_Geography:City_Country a DPDO:Attribute ;
    DPDO:isConvergence false ;
    DPDO:isCrossDimensional false ;
    DPDO:isDescriptive false ;
    DPDO:isOptional false ;
    DPDO:isShared false ;
    DPDO:itemName "Country"^^xsd:string .

LLM4BI_Geography:City_County a DPDO:Attribute ;
    DPDO:isConvergence false ;
    DPDO:isCrossDimensional false ;
    DPDO:isDescriptive false ;
    DPDO:isOptional true ;
    DPDO:isShared false ;
    DPDO:itemName "County"^^xsd:string .

LLM4BI_Geography:City_State a DPDO:Attribute ;
    DPDO:isConvergence false ;
    DPDO:isCrossDimensional false ;
    DPDO:isDescriptive false ;
    DPDO:isOptional true ;
    DPDO:isShared false ;
    DPDO:itemName "State"^^xsd:string .

