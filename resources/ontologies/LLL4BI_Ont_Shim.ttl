@prefix LLM4BI: <http://www.foo.bar/LLM4BI/ontologies/LLM4BI#> .
@prefix llm:    <http://www.foo.bar/LLM4BI/ontologies/LLM4BI#> .
@prefix DPDO:   <http://www.foo.bar/LLM4BI/ontologies/LLM4BI#> .  # map DPDO to same ns so domain/range resolve
@prefix owl:    <http://www.w3.org/2002/07/owl#> .
@prefix rdf:    <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:   <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd:    <http://www.w3.org/2001/XMLSchema#> .

# Classes (mirror prof’s IRIs, safe to redeclare)
LLM4BI:BIObject  a owl:Class .
LLM4BI:Fact      a owl:Class ; rdfs:subClassOf LLM4BI:BIObject .
LLM4BI:Measure   a owl:Class ; rdfs:subClassOf LLM4BI:BIObject .
LLM4BI:Attribute a owl:Class ; rdfs:subClassOf LLM4BI:BIObject .
LLM4BI:Arc       a owl:Class .

# determines is an OBJECT property between BIObjects
LLM4BI:determines a owl:ObjectProperty ;
  rdfs:domain LLM4BI:BIObject ;
  rdfs:range  LLM4BI:BIObject ;
  rdfs:label "determines (child → parent in attribute hierarchies)"@en .

# Arc linking properties
LLM4BI:source a owl:ObjectProperty ;
  rdfs:domain LLM4BI:Arc ;
  rdfs:range  LLM4BI:BIObject .

LLM4BI:target a owl:ObjectProperty ;
  rdfs:domain LLM4BI:Arc ;
  rdfs:range  LLM4BI:BIObject .

# Datatype props used by your generator (safe to declare here)
LLM4BI:itemName              a owl:DatatypeProperty ; rdfs:range xsd:string .
LLM4BI:itemID                a owl:DatatypeProperty ; rdfs:range xsd:string .
LLM4BI:Description           a owl:DatatypeProperty ; rdfs:range xsd:string .
LLM4BI:Formula               a owl:DatatypeProperty ; rdfs:range xsd:string .
LLM4BI:AggregationNotes      a owl:DatatypeProperty ; rdfs:range xsd:string .
LLM4BI:belongsToFactSchema   a owl:DatatypeProperty ; rdfs:range xsd:string .
LLM4BI:hierarchyName         a owl:DatatypeProperty ; rdfs:range xsd:string .
