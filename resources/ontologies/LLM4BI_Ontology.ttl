@base <http://www.foo.bar/LLM4BI/ontologies/DPDO> .

<http://www.foo.bar/LLM4BI/ontologies/LLM4BI> rdf:type owl:Ontology .

LLM4BI:BIObject rdf:type owl:Class .

# Concettualmente la FT non pu√≤ essere optional, TODO
LLM4BI:Fact rdf:type owl:Class ;
    rdfs:subClassOf LLM4BI:BIObject .

LLM4BI:Measure rdf:type owl:Class ;
    rdfs:subClassOf LLM4BI:BIObject .
            
LLM4BI:Attribute rdf:type owl:Class ;
    rdfs:subClassOf LLM4BI:BIObject .

# Modelling arcs
LLM4BI:determines rdf:type rdf:type owl:Property ;
    rdfs:subPropertyOf owl:topDataProperty ;
    rdfs:domain DPDO:BIObject ;
    rdfs:range DPDO:BIObject.


