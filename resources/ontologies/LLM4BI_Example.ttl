@prefix LLM4BI_Example: <http://www.foo.bar/LLM4BI/ontologies/LLM4BI_Example#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix LLM4BI: <http://www.foo.bar/LLM4BI/ontologies/LLM4BI#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix swrl: <http://www.w3.org/2003/11/swrl#> .
@prefix swrlb: <http://www.w3.org/2003/11/swrlb#> .
@prefix spin:    <http://spinrdf.org/spin#> .
@prefix spl:     <http://spinrdf.org/spl#> .
@base <http://www.foo.bar/LLM4BI/ontologies/LLM4BI_Example> .

LLM4BI_Example: a owl:Ontology .

## Template for nodes name
# LLM4BI_Example:{FACT_NAME}_{NODE_NAME}

## Template for edges name
# LLM4BI_Example:{FACT_NAME}_{SOURCE_NODE_NAME}_{DEST_NODE:NAME}


# Fact table
LLM4BI_Example:HistoricalOrder a LLM4BI:Fact ;
    LLM4BI_Example:HistoricalOrder_HistoricalOrder_Customer LLM4BI_Example:HistoricalOrder_Customer ;
    LLM4BI_Example:HistoricalOrder_HistoricalOrder_Quantity LLM4BI_Example:HistoricalOrder_Quantity .

# Measure
LLM4BI_Example:HistoricalOrder_Quantity a LLM4BI:Measure ;
    LLM4BI:AggregationLevel ("SUM" "AVG") .

# Customer dimension
LLM4BI_Example:HistoricalOrder_Customer a LLM4BI:Attribute ;
    LLM4BI_Example:HistoricalOrder_Customer_CustomerCountry LLM4BI_Example:HistoricalOrder_CustomerCountry .

LLM4BI_Example:HistoricalOrder_CustomerCountry a LLM4BI:Attribute ;
    rdfs:label: "The nation of a city" ;
    LLM4BI:LogicalName "COUNTRY" ;
    LLM4BI:SampleValues ("Italy" "UK" "USA") ;
    LLM4BI_Example:CustomerCountry_CustomerArea LLM4BI_Example:CustomerArea .
    
LLM4BI_Example:HistoricalOrder_CustomerArea a LLM4BI:Attribute .

# Customer dimension dependencies
LLM4BI_Example:HistoricalOrder_HistoricalOrder_Customer a LLM4BI:Dependency .

LLM4BI_Example:HistoricalOrder_Customer_CustomerCountry a LLM4BI:Dependency .

LLM4BI_Example:HistoricalOrder_CustomerCountry_CustomerArea a LLM4BI:Dependency;
    LLM4BI:isOptional "true"^^xsd:boolean .

# Measure dependency
LLM4BI_Example:HistoricalOrder_HistoricalOrder_Quantity a LLM4BI:Dependency .